{
  "rewrites": [
    {
      "source": "/((?!static|favicon.ico|manifest.json|robots.txt|.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|pdf)).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}